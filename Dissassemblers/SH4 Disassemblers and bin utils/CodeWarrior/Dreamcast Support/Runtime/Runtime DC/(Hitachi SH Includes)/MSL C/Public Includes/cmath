	inline double fdim(double x, double y)
		{return (double)fdim((long double)x, (long double)y);}
	inline double fmax(double x, double y)
		{return (double)fmax((long double)x, (long double)y);}
	inline double fmin(double x, double y)
		{return (double)fmin((long double)x, (long double)y);}
//	inline double fma(double x, double y, double z)
//		{return (double)((long double)x*y + z);}

	extern "C" {
#endif

/*  foo redirection (everything but 68K) */
#endif  /*  __FP__  */
#ifndef __MC68K__

	#ifndef __cplusplus

		#ifdef __POWERPC__
			double fabs(double x);
			#define fabs(x) __fabs(x)
		#endif
		double scalbn(double x, int n);
		double scalbln(double x, long n);
		long int lrint(double x);
		#ifdef __MSL_LONGLONG_SUPPORT__		/* as 981014 */
			long long llrint(double x);
			long long llround(double x);
		#endif /* __MSL_LONGLONG_SUPPORT__ */
		long int lround(double x);
		#if __dest_os == __mac_os
			double nextafter(double x, double y);
			#define nextafter(x, y) nextafterd(x, y)
		#endif
	/*	double fma(double x, double y, double z); */

		#define scalbn(x, n)  scalb(x, n)
		#define scalbln(x, n) scalb(x, n)
		#define lrint(x)      rinttol(x)
		#ifdef __MSL_LONGLONG_SUPPORT__		/* as 981014 */
			#define llrint(x)     ((long long)rint(x))
			#define llround(x)    ((long long)round(x))
		#endif /* __MSL_LONGLONG_SUPPORT__ */
		#define lround(x)     roundtol(x)
	
	/*	#define fma(x, y, z)  ((double)((long double)(x)*(y) + (z))) */

	#else

		} // extern "C"

		#ifdef __POWERPC__
			inline double fabs(double x)
				{return __fabs(x);}
		#endif
		inline double scalbn(double x, int n)
			{return (double)scalb(x, n);}
		inline double scalbln(double x, long n)
			{return (double)scalb(x, n);}
		inline long int lrint(double x)
			{return rinttol(x);}
		#ifdef __MSL_LONGLONG_SUPPORT__		/* as 981014 */
			inline long long llrint(double x)
				{return (long long)rint(x);}
			inline long long llround(double x)
				{return (long long)round(x);}
		#endif /* __MSL_LONGLONG_SUPPORT__ */		
		inline long int lround(double x)
			{return roundtol(x);}
		#if __dest_os == __mac_os
			inline double nextafter(double x, double y)
				{return nextafterd(x, y);}
		#endif
	//	inline double fma(double x, double y, double z)
	//		{return (double)((long double)x*y + z);}

		extern "C" {

	#endif

#endif  /* __FP__ */

#ifdef __MC68K__
	#if (__dest_os==__mac_os  && defined(__FP__))
		#ifndef __cplusplus
			long int lrint(double x);
			long long llrint(double x);
			long int lround(double x);
